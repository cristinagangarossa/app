package com.neperiagroup.happysalus.utility;

public class TagSettings {

    private static final String TAG = TagUser.class.getName();

    private String KEY_INCOMING_CALL = TAG + ".incoming_call";
    private String KEY_WHATSAPP = TAG + ".whatsapp";
    private String KEY_INCOMING_EMAIL= TAG + ".incoming_email";
    private String KEY_GOL_NOTIFICATIONS= TAG + ".gol_notifications";
    private String KEY_HEALTH_HISTORY = TAG + ".health_history";

    public TagSettings(){}

    public String getKEY_INCOMING_CALL() {
        return KEY_INCOMING_CALL;
    }

    public void setKEY_INCOMING_CALL(String KEY_INCOMING_CALL) {
        this.KEY_INCOMING_CALL = KEY_INCOMING_CALL;
    }

    public String getKEY_WHATSAPP() {
        return KEY_WHATSAPP;
    }

    public void setKEY_WHATSAPP(String KEY_WHATSAPP) {
        this.KEY_WHATSAPP = KEY_WHATSAPP;
    }

    public String getKEY_INCOMING_EMAIL() {
        return KEY_INCOMING_EMAIL;
    }

    public void setKEY_INCOMING_EMAIL(String KEY_INCOMING_EMAIL) {
        this.KEY_INCOMING_EMAIL = KEY_INCOMING_EMAIL;
    }

    public String getKEY_GOL_NOTIFICATIONS() {
        return KEY_GOL_NOTIFICATIONS;
    }

    public void setKEY_GOL_NOTIFICATIONS(String KEY_GOL_NOTIFICATIONS) {
        this.KEY_GOL_NOTIFICATIONS = KEY_GOL_NOTIFICATIONS;
    }

    public String getKEY_HEALTH_HISTORY() {
        return KEY_HEALTH_HISTORY;
    }

    public void setKEY_HEALTH_HISTORY(String KEY_HEALTH_HISTORY) {
        this.KEY_HEALTH_HISTORY = KEY_HEALTH_HISTORY;
    }
}
